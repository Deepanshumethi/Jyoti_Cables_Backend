{
  "version": 2,
  "builds": [
    {
      "src": "SendMail/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "./build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "SendMail/wsgi.py"
    }
  ]
}


